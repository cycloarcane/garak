{
  "rest": {
    "RestGenerator": {
      "name": "oobabooga Local Model",
      "uri": "http://127.0.0.1:5000/v1/completions",
      "method": "post",
      "headers": {
        "Content-Type": "application/json"
      },
      "req_template_json_object": {
        "prompt": "$INPUT",
        "max_tokens": 1000,
        "temperature": 0.7
      },
      "response_json": true,
      "response_json_field": "choices[0].text"
    }
  }
}
